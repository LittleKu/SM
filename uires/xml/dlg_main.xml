<SOUI
  name="mainWindow"
  title="@string/title"
  bigIcon="ICON_LOGO:32"
  smallIcon="ICON_LOGO:16"
  width="600"
  height="400"
  margin="3,3,3,3"
  resizable="1"
  wndType="appMain"
  appWnd="1"
  sendWheel2Hover="1">
  <root skin="_skin.sys.wnd.bkgnd" cache="1">
    <caption pos="0,0,-0,30" show="1" font="adding:0">
      <icon pos="10,8" src="ICON_LOGO:16"/>
      <text pos="29,9">@string/title</text>
      <imgbtn name="btn_close" skin="_skin.sys.btn.close"    pos="-45,0" tip="close" animate="1"/>
      <imgbtn name="btn_min" skin="_skin.sys.btn.minimize" pos="-83,0" animate="1" />
    </caption>
    <sm_webview pos="5,[2,-5,-5" url="https://www.baidu.com/">
    </sm_webview>
  </root>
</SOUI>
